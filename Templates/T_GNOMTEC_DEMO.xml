<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/ ..\FrameXML\UI.xsd">

	<!-- GnomTEC Templates Demonstration -->
	<Frame name="T_GnomTEC_Demo_Window" inherits="T_GNOMTEC_FRAME_WINDOW" movable="true" resizable="true" hidden="true">
		<Size x="200" y="200" />
		<Frames>
			<Frame name="$parent_Header" inherits="T_GNOMTEC_FRAME_WINDOW_HEADER">
			</Frame>
			<Frame name="$parent_InnerFrame" inherits="T_GNOMTEC_FRAME_WINDOW_INNERFRAME">
				<Frames>
					<Frame name="$parent_Container" inherits="T_GNOMTEC_FRAME_CONTAINER_TABULATOR">
						<Frames>
							<Frame name="$parent_Tabulator" inherits="T_GNOMTEC_FRAME_CONTAINER_TABULATOR_HORIZONTAL">
								<Anchors>
									<Anchor point="TOP"/>
								</Anchors>
								<Frames>
									<Button name="$parent_Tab1" inherits="T_GNOMTEC_BUTTON_TABULATOR" id="1">
										<Size x="80"/>
										<Anchors>
											<Anchor point="LEFT"/>
										</Anchors>
									</Button>
									<Button name="$parent_Tab2" inherits="T_GNOMTEC_BUTTON_TABULATOR" id="2">
										<Size x="80"/>
										<Anchors>
											<Anchor point="LEFT" relativeTo="$parent_Tab1" relativePoint="RIGHT"/>
										</Anchors>
									</Button>
									<Button name="$parent_Tab3" inherits="T_GNOMTEC_BUTTON_TABULATOR" id="3">
										<Size x="80"/>
										<Anchors>
											<Anchor point="LEFT" relativeTo="$parent_Tab2" relativePoint="RIGHT"/>
										</Anchors>
									</Button>
									<Button name="$parent_Tab4" inherits="T_GNOMTEC_BUTTON_TABULATOR" id="4">
										<Size x="80"/>
										<Anchors>
											<Anchor point="LEFT" relativeTo="$parent_Tab3" relativePoint="RIGHT"/>
										</Anchors>
									</Button>
									<Frame name="$parent_Spacer" inherits="T_GNOMTEC_FRAME_SPACER">
										<Anchors>
											<Anchor point="LEFT" relativeTo="$parent_Tab4" relativePoint="RIGHT"/>
											<Anchor point="RIGHT"/>
										</Anchors>
									</Frame>
								</Frames>
							</Frame>
							<Frame name="$parent_InnerFrame" inherits="T_GNOMTEC_FRAME_CONTAINER_TABULATOR_INNERFRAME">
								<Anchors>
									<Anchor point="TOP" relativeTo="$parent_Tabulator" relativePoint="BOTTOM"/>
									<Anchor point="LEFT"/>
									<Anchor point="RIGHT"/>
									<Anchor point="BOTTOM"/>							
								</Anchors>
								<Frames>
									<Frame name="$parent_Map" inherits="T_GNOMTEC_FRAME_MAP" id="1">
										<Anchors>
											<Anchor point="TOP" y="-5"/>
											<Anchor point="LEFT" x="5"/>
											<Anchor point="RIGHT" x="-5"/>
											<Anchor point="BOTTOM" y="5"/>							
										</Anchors>
									</Frame>
									<ScrollFrame name="$parent_EditBox" inherits="T_GNOMTEC_SCROLLFRAME_EDITBOX"  id="2">
										<Anchors>
											<Anchor point="TOP" y="-5"/>
											<Anchor point="LEFT" x="5"/>
											<Anchor point="RIGHT" x="-5"/>
											<Anchor point="BOTTOM" y="5"/>							
										</Anchors>
									</ScrollFrame>
									<Frame name="$parent_Messages" inherits="T_GNOMTEC_FRAME_SCROLLINGMESSAGE" id="3">
										<Anchors>
											<Anchor point="TOP" y="-5"/>
											<Anchor point="LEFT" x="5"/>
											<Anchor point="RIGHT" x="-5"/>
											<Anchor point="BOTTOM" y="5"/>							
										</Anchors>
									</Frame>
									
								</Frames>
							</Frame>
						</Frames>
					</Frame>
				</Frames>
			</Frame>
			<Frame name="$parent_Footer" inherits="T_GNOMTEC_FRAME_WINDOW_FOOTER">
			</Frame>
		</Frames>
	</Frame>


</Ui>