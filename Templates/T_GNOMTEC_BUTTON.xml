<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/ ..\FrameXML\UI.xsd">

	<!-- T_GNOMTEC_BUTTON_TABULATOR: tabs of a horizontal tabulator container. -->
	<Button name="T_GNOMTEC_BUTTON_TABULATOR" virtual="true">
		<Anchors>
			<Anchor point="TOP"/>
			<Anchor point="BOTTOM"/>
		</Anchors>
		<Layers>
			<Layer>
				<FontString name="$parent_Title" inherits="GameFontNormalLeft" justifyH="CENTER" text="TAB">
					<Color r="1.0" g="1.0" b="1.0"/>
					<Anchors>
						<Anchor point="CENTER" y="-2"/>
					</Anchors>
				</FontString>
			</Layer>
		</Layers>
		<NormalTexture file="Interface\CHATFRAME\ChatFrameTab">
			<TexCoords left="0" right="1" top="0.3" bottom="1"/>
		</NormalTexture>
		<HighlightTexture file="Interface\QuestFrame\UI-QuestTitleHighlight" alphaMode="ADD"/>
		<Scripts>
			<OnLoad>
				self.GnomTEC_ChangeSize = T_GNOMTEC_BUTTON_TABULATOR_ChangeSize;
			</OnLoad>
			<OnClick>
				T_GNOMTEC_BUTTON_TABULATOR_OnClick(self,self:GetID());
			</OnClick>
		</Scripts>
	</Button>
</Ui>
