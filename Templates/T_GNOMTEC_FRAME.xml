	<!-- T_GNOMTEC_FRAME_CONTAINER_TABLE_SCROLLBAR: scrollbar of a table container. -->
	<Frame name="T_GNOMTEC_FRAME_CONTAINER_TABLE_SCROLLBAR" parentKey="GnomTEC_Scrollbar" virtual="true">
		<Size x="16" y="16" />
		<Anchors>
			<Anchor point="TOPRIGHT" y="-24"/>
			<Anchor point="BOTTOMRIGHT"/>
		</Anchors>
		<Frames>
			<Slider name="$parent_Slider" resizable="true" setAllPoints="true" parentKey="slider">
				<Anchors>
					<Anchor point="TOPLEFT">
						<Offset x="0" y="-16" />
					</Anchor>
					<Anchor point="BOTTOMRIGHT">
						<Offset x="0" y="16" />
					</Anchor>
				</Anchors>	
				<Size>
					<AbsDimension x="16" y="43"/>
				</Size>
				<Scripts>
					<OnValueChanged>
						T_GNOMTEC_FRAME_CONTAINER_TABLE_SCROLLBAR_SliderOnValueChanged(self:GetParent(), value)
					</OnValueChanged>
					<OnMouseWheel>
						self:SetValue(self:GetValue() - delta);
					</OnMouseWheel>
					<OnEnter>
						local parent = self:GetParent();
						T_GNOMTEC_FRAME_CONTAINER_TABLE_SCROLLBAR_SetSlider(parent)
					</OnEnter>
				</Scripts>
				<ThumbTexture name="$parentThumbTexture" inherits="UIPanelScrollBarButton" file="Interface\Buttons\UI-ScrollBar-Knob">
					<Size>
						<AbsDimension x="16" y="16"/>
					</Size>
				</ThumbTexture>
			</Slider>
			<Button name="$parent_ScrollUpButton" inherits="UIPanelScrollUpButtonTemplate">
				<Anchors>
					<Anchor point="BOTTOM" relativePoint="TOP" relativeTo="$parent_Slider"/>
				</Anchors>
				<Scripts>
					<OnClick>
						local parent = self:GetParent();
						parent.slider:SetValue(parent.slider:GetValue() - 1);
						T_GNOMTEC_FRAME_CONTAINER_TABLE_SCROLLBAR_SetSlider(parent)
						PlaySound("UChatScrollButton");
					</OnClick>
					<OnEnter>
						local parent = self:GetParent();
						T_GNOMTEC_FRAME_CONTAINER_TABLE_SCROLLBAR_SetSlider(parent)
						PlaySound("UChatScrollButton");
					</OnEnter>
				</Scripts>
			</Button>
			<Button name="$parent_ScrollDownButton" inherits="UIPanelScrollDownButtonTemplate">
				<Anchors>
					<Anchor point="TOP" relativePoint="BOTTOM" relativeTo="$parent_Slider"/>
				</Anchors>	
				<Scripts>
					<OnClick>
						local parent = self:GetParent();
						parent.slider:SetValue(parent.slider:GetValue() + 1);
						T_GNOMTEC_FRAME_CONTAINER_TABLE_SCROLLBAR_SetSlider(parent)
						PlaySound("UChatScrollButton");
					</OnClick>
					<OnEnter>
						local parent = self:GetParent();
						T_GNOMTEC_FRAME_CONTAINER_TABLE_SCROLLBAR_SetSlider(parent)
						PlaySound("UChatScrollButton");
					</OnEnter>
				</Scripts>
			</Button>
		</Frames>
		<Scripts>
			<OnLoad>
				self.GnomTEC_TriggerResize = T_GNOMTEC_FRAME_TriggerResize
				self.GnomTEC_ChangeSize = T_GNOMTEC_FRAME_CONTAINER_TABLE_SCROLLBAR_ChangeSize;
				T_GNOMTEC_FRAME_CONTAINER_TABLE_SCROLLBAR_SetSlider(self)
			</OnLoad>
		</Scripts>
	</Frame>

	
