	<!-- T_GNOMTEC_FRAME_SCROLLINGMESSAGE: scrollable area with messages -->
	<Frame name="T_GNOMTEC_FRAME_SCROLLINGMESSAGE" virtual="true">
		<Frames>
			<ScrollingMessageFrame name="$parent_Text" fade="false" maxLines="1024" parentKey="messages">
				<Anchors>
					<Anchor point="TOPLEFT">
						<Offset x="0" y="0" />
					</Anchor>
					<Anchor point="BOTTOMRIGHT">
						<Offset x="-16" y="0" />
					</Anchor>
				</Anchors>	
				<FontString inherits="ChatFontNormal" justifyH="LEFT" indented="true" nonspacewrap="true"/>
				<Scripts>
					<OnMouseWheel>
						self:GetParent().slider:SetValue(self:GetParent().slider:GetValue() - delta);
					</OnMouseWheel>
				</Scripts>
			</ScrollingMessageFrame>
			<Slider name="$parent_Slider" resizable="true" setAllPoints="true" parentKey="slider">
				<Anchors>
					<Anchor point="TOPRIGHT">
						<Offset x="0" y="-16" />
					</Anchor>
					<Anchor point="BOTTOMRIGHT">
						<Offset x="0" y="16" />
					</Anchor>
				</Anchors>	
				<Size>
					<AbsDimension x="16" y="43"/>
				</Size>
				<Frames>
					<Button name="$parentScrollUpButton" inherits="UIPanelScrollUpButtonTemplate">
						<Anchors>
							<Anchor point="BOTTOM" relativePoint="TOP"/>
						</Anchors>
						<Scripts>
							<OnClick>
								local parent = self:GetParent();
								parentOfParent = parent:GetParent();
								parentOfParent.messages:ScrollUp()
								T_GNOMTEC_FRAME_SCROLLINGMESSAGE_SetSlider(parentOfParent)
								PlaySound("UChatScrollButton");
							</OnClick>
							<OnEnter>
								local parent = self:GetParent();
								parentOfParent = parent:GetParent();
								T_GNOMTEC_FRAME_SCROLLINGMESSAGE_SetSlider(parentOfParent)
							</OnEnter>
						</Scripts>
					</Button>
					<Button name="$parentScrollDownButton" inherits="UIPanelScrollDownButtonTemplate">
						<Anchors>
							<Anchor point="TOP" relativePoint="BOTTOM"/>
						</Anchors>	
						<Scripts>
							<OnClick>
								local parent = self:GetParent();
								parentOfParent = parent:GetParent();
								parentOfParent.messages:ScrollDown()
								T_GNOMTEC_FRAME_SCROLLINGMESSAGE_SetSlider(parentOfParent)
								PlaySound("UChatScrollButton");
							</OnClick>
							<OnEnter>
								local parent = self:GetParent();
								parentOfParent = parent:GetParent();
								T_GNOMTEC_FRAME_SCROLLINGMESSAGE_SetSlider(parentOfParent)
							</OnEnter>
						</Scripts>
					</Button>
				</Frames>
				<Scripts>
					<OnValueChanged>
						T_GNOMTEC_FRAME_SCROLLINGMESSAGE_SliderOnValueChanged(self:GetParent(), value)
					</OnValueChanged>
					<OnMouseWheel>
						self:SetValue(self:GetValue() - delta);
					</OnMouseWheel>
					<OnEnter>
						local parent = self:GetParent();
						T_GNOMTEC_FRAME_SCROLLINGMESSAGE_SetSlider(parent)
					</OnEnter>
				</Scripts>
				<ThumbTexture name="$parentThumbTexture" inherits="UIPanelScrollBarButton" file="Interface\Buttons\UI-ScrollBar-Knob">
					<Size>
						<AbsDimension x="16" y="16"/>
					</Size>
				</ThumbTexture>
			</Slider>
		</Frames>
		<Scripts>
			<OnLoad>
				self.GnomTEC_ChangeSize = T_GNOMTEC_FRAME_SCROLLINGMESSAGE_ChangeSize;
				T_GNOMTEC_FRAME_SCROLLINGMESSAGE_SetSlider(self)
			</OnLoad>
			<OnEnter>
				T_GNOMTEC_FRAME_SCROLLINGMESSAGE_SetSlider(self)
			</OnEnter>
		</Scripts>
	</Frame>
		
