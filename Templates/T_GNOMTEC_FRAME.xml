<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/ ..\FrameXML\UI.xsd">

	<!-- T_GNOMTEC_FRAME_TOOLTIP: Empty tooltip which could be moveable and resizable. -->
	<Frame name="T_GNOMTEC_FRAME_TOOLTIP" parent="UIParent" toplevel="true" clampedToScreen="true" movable="true" resizable="true" hidden="true" virtual="true">
		<Size x="128" y="128" />
		<ResizeBounds>
         <minResize>
         	<AbsDimension x="64" y="64"/>
      	</minResize>
      </ResizeBounds>
		<Anchors>
			<Anchor point="CENTER" relativePoint="CENTER"/>
		</Anchors>
		<Backdrop bgFile="Interface\Addons\GnomTEC_Assistant\Textures\UI-Tooltip-Background" edgeFile="Interface\Addons\GnomTEC_Assistant\Textures\UI-Tooltip-Border" tile="true">
			<EdgeSize val="16"/>
			<TileSize val="16"/>
			<Color r="0.25" g="0.25" b="0.25"/>
			<BackgroundInsets left="5" right="5" top="5" bottom="5"/>
		</Backdrop>
		<Frames>
			<Button name="$parent_Resize">
				<Size x="16" y="16"/>
				<Anchors>
					<Anchor point="BOTTOMRIGHT">
						<Offset>
							<AbsDimension x="0" y="0"/>
						</Offset>
					</Anchor>
				</Anchors>
				<Scripts>
					<OnMouseDown>
						if (self:GetParent():IsResizable()) then
							self:GetParent():StartSizing();
						end
					</OnMouseDown>
					<OnMouseUp>
						self:GetParent():StopMovingOrSizing();
					</OnMouseUp>
				</Scripts>
				<NormalTexture file="Interface\ChatFrame\UI-ChatIM-SizeGrabber-Up"/>
				<HighlightTexture file="Interface\ChatFrame\UI-ChatIM-SizeGrabber-Highlight"/>
				<PushedTexture file="Interface\ChatFrame\UI-ChatIM-SizeGrabber-Down"/>
			</Button>
		</Frames>
		<Scripts>
			<OnLoad>
				self.GnomTEC_TriggerResize = T_GNOMTEC_FRAME_Resize
				self.GnomTEC_Resize = getglobal(self:GetName().."_Resize")
				self.GnomTEC_Width = self:GetWidth()
				self.GnomTEC_Height = self:GetHeight()
			</OnLoad>
			<OnShow>
				if (self:IsResizable()) then
					self.GnomTEC_Resize:Show();
				else
					self.GnomTEC_Resize:Hide();
				end
				T_GNOMTEC_FRAME_Resize(self, 0, 0)
			</OnShow>
			<OnMouseDown>
				if (self:IsMovable()) then
					self:StartMoving();
				end
			</OnMouseDown>
			<OnMouseUp>self:StopMovingOrSizing();</OnMouseUp>
			<OnSizeChanged>
				if ((not self.GnomTEC_Width) or (not self.GnomTEC_Height)) then
					self.GnomTEC_Width = self:GetWidth()
					self.GnomTEC_Height = self:GetHeight()
				else
					T_GNOMTEC_FRAME_Resize(self, self:GetWidth()-self.GnomTEC_Width, self:GetHeight()-self.GnomTEC_Height)
					self.GnomTEC_Width = self:GetWidth()
					self.GnomTEC_Height = self:GetHeight()
				end				
			</OnSizeChanged>
		</Scripts>
	</Frame>

